{
  "name": "core",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "install": "dotnet restore",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "edge": "start microsoft-edge:http://localhost:5050",
    "chrome": "start chrome http://localhost:5050",
    "firefox": "start firefox http://localhost:5050",
    "publish": "npm run restore && npm run clean:wwwroot && dotnet publish --configuration Release -r ubuntu.16.04-x64 --output \"./wwwroot\" && ng build --prod && npm run copy:dist",
    "clean:wwwroot": "del /q .\\wwwroot\\*.* && for /d %i in (.\\wwwroot\\*.*) do @rmdir /s /q \"%i\" ",
    "copy:dist": "cpx \"dist/**/*\" wwwroot",
    "updateNPM": "npm i -g npm-check-updates && ncu -u -a && npm update"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.1.1",
    "@angular/compiler": "^4.1.1",
    "@angular/core": "^4.1.1",
    "@angular/forms": "^4.1.1",
    "@angular/http": "^4.1.1",
    "@angular/platform-browser": "^4.1.1",
    "@angular/platform-browser-dynamic": "^4.1.1",
    "@angular/router": "^4.1.1",
    "core-js": "^2.4.1",
    "rxjs": "^5.3.1",
    "zone.js": "^0.8.10",
    "bootstrap": "^3.3.7"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.2",
    "@angular/compiler-cli": "^4.1.1",
    "@types/bootstrap": "^3.3.33",
    "@types/jasmine": "^2.5.47",
    "@types/lodash": "^4.14.63",
    "@types/pixi.js": "^4.4.4",
    "@types/node": "^7.0.18",
    "@types/jquery": "^2.0.43",
    "codelyzer": "^3.0.1",
    "cpx": "^1.5.0",
    "jasmine-core": "^2.6.1",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.1.1",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "typescript": "~2.3.2",
    "concurrently": "^3.4.0",
    "rxjs": "^5.3.0",
    "zone.js": "^0.8.8",
    "bootstrap": "^3.3.7",
    "lodash": "^4.17.4",
    "pixi.js": "^4.5.1",
    "jquery": "^3.2.1"
  }
}